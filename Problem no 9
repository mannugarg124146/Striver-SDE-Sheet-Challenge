vector<int> ninjaAndSortedArrays(vector<int>& arr1, vector<int>& arr2, int m, int n) {

	// Write your code here.
	 vector<int>num3;
        for(int i=0;i<m;i++)
        {
            num3.push_back(arr1[i]);
        }
         for(int i=0;i<n;i++)
        {
            num3.push_back(arr2[i]);
        }
        sort(num3.begin(),num3.end());
        return num3;

}
