int findDuplicate(vector<int> &arr, int n){
	// Write your code here.
	 //int n=arr.size();
        sort(arr.begin(),arr.end());
        int duplicate;
        for(int i=0;i<n-1;i++)
        {
              if(arr[i]==arr[i+1])
              {
                  duplicate=arr[i];
                  break;

              }
             
        }
        return duplicate;
}
